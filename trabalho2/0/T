{
    cylinder1
    {
        type            fixedValue;
        value           uniform 473;
    }

    cylinder2
    {
        type            fixedValue;
        value           uniform 373;
    }

    down
    {
        type            zeroGradient;
    }

    up
    {
        type            zeroGradient;
    }

    right
    {
        type            zeroGradient;
    }

    left
    {
        type            zeroGradient;
    }

}

