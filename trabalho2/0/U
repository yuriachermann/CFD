FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (7.5 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (7.5 0 0); // Velocidade de entrada
    }
    outlet
    {
        type            zeroGradient; // Sem condição de contorno específica na saída
    }
    frontAndBack
    {
        type            empty;
    }
    walls
    {
        type            noSlip;
    }
}

// ************************************************************************* //
